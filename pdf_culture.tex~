--select code_cultu, avg(pc_sau) from s_enti where code_cultu is not null and code_cultu='C1' and pc_sau > 4 group by code_cultu order by code_cultu;
select code_cultu, min(pc_sau), max(pc_sau),avg(pc_sau), (select count(pc_sau) from s_enti where code_cultu is not null and code_cultu='C1' and pc_sau <= 2 group by code_cultu order by code_cultu)  from s_enti where code_cultu is not null and code_cultu='C1' group by code_cultu order by code_cultu;
\end{verbatim}

